pipeline {
    agent any
    stages {
        stage('Example') {
            steps {
                sh 'git clone https://github.com/ruchi672/projectRepository.git'
            }
        }
	
	stage('Sonar Analysis') {
            steps {
                withSonarQubeEnv('sonar') {
               sh 'mvn clean verify sonar:sonar -Dsonar.projectKey=sonar -Dsonar.host.url=http://34.100.252.55:9000 -Dsonar.login=admin -Dsonar.password=admin'
             }
            }
        }
    }
     post {
        always {
            cleanWs()
        }
    }
}
